{
    "name": "nilsh.ch",
    "version": "0.4.1",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint --fix",
        "prettier-check": "prettier . --check",
        "prettier-write": "prettier . --write",
        "cf-build": "opennextjs-cloudflare build",
        "cf-preview": "npm run cf-build && opennextjs-cloudflare preview",
        "cf-deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
        "cf-deploy-preview": "npx wrangler versions upload",
        "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
    },
    "dependencies": {
        "@opennextjs/cloudflare": "^1",
        "dayjs": "^1",
        "next": "^15",
        "react": "^19",
        "react-dom": "^19"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@eslint/js": "^9",
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8",
        "@typescript-eslint/parser": "^8",
        "autoprefixer": "^10",
        "daisyui": "^5",
        "esbuild": "^0.27",
        "eslint": "^9",
        "eslint-config-next": "^15",
        "eslint-config-prettier": "^10",
        "eslint-plugin-prettier": "^5",
        "import-sort-style-module": "^6",
        "postcss": "^8",
        "prettier": "^3",
        "prettier-plugin-import-sort": "latest",
        "prettier-plugin-tailwindcss": "latest",
        "tailwindcss": "^4",
        "typescript": "^5",
        "typescript-eslint": "^8",
        "wrangler": "^4"
    },
    "importSort": {
        ".js, .jsx, .ts, .tsx": {
            "style": "module",
            "parser": "typescript"
        }
    }
}
